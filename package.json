{
  "name": "dataclouder-template",
  "config": {
    "appId": "dev.dataclouder.template",
    "appIdDev": "dev.dataclouder.template.dev"
  },
  "version": "0.0.10",
  "author": "Ionic Framework",
  "homepage": "dataclouder.dev",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:qa": "ng serve -c qa",
    "start:prod": "ng serve -c production",
    "build": "ng build",
    "build:dev": "ng build",
    "build:prod": "ng build -c production",
    "watch": "ng build --watch --configuration development",
    "update:dc": "npm install @dataclouder/app-auth@latest @dataclouder/conversation-system@latest @dataclouder/lessons@latest @dataclouder/storage-uploader@latest @dataclouder/core-components@latest @dataclouder/dc-mic@latest",
    "android:debug": "ionic cap run android -l --external",
    "deploy": "npm run build && firebase deploy --only hosting:dataclouder-dev",
    "prebuild": "npm version patch --no-git-tag-version && node scripts/sync-version.js",
    "ios:dev": "ng build -c development && cap copy ios && npx cap run ios",
    "ios:prod": "ng build -c production && cap copy ios && npx cap run ios --configuration Release",
    "ios:dev-sim": "ng build -c development && cap copy ios && npx cap run ios --target 3FFDE946-E48B-4215-A7CD-D7C1872FDAF2",
    "android:dev": "ng build -c development && cap copy android && cd android && ./gradlew assembleDev && adb install app/build/outputs/apk/dev/app-dev.apk && adb shell monkey -p dev.dataclouder.template.dev -c android.intent.category.LAUNCHER 1",
    "android:pro": "ng build -c production && cap copy android && cd android && ./gradlew assembleRelease && adb install app/build/outputs/apk/release/app-release.apk && adb shell monkey -p ${npm_package_config_appId} -c android.intent.category.LAUNCHER 1",
    "test": "ng test",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.2",
    "@angular/cdk": "^19.1.0",
    "@angular/common": "^19.1.2",
    "@angular/compiler": "^19.1.2",
    "@angular/core": "^19.1.2",
    "@angular/fire": "^19.0.0",
    "@angular/forms": "^19.1.2",
    "@angular/platform-browser": "^19.1.2",
    "@angular/platform-browser-dynamic": "^19.1.2",
    "@angular/router": "^19.1.2",
    "@capacitor-community/apple-sign-in": "^6.0.0",
    "@capacitor-community/speech-recognition": "^6.0.1",
    "@capacitor/android": "6.1.2",
    "@capacitor/app": "6.0.1",
    "@capacitor/core": "6.1.2",
    "@capacitor/haptics": "6.0.1",
    "@capacitor/ios": "^6.1.2",
    "@capacitor/keyboard": "6.0.2",
    "@capacitor/status-bar": "6.0.1",
    "@ckeditor/ckeditor5-angular": "^6.0.1",
    "@ckeditor/ckeditor5-build-balloon-block": "^38.1.0",
    "@codetrix-studio/capacitor-google-auth": "^3.4.0-rc.4",
    "@dataclouder/app-auth": "^0.0.36",
    "@dataclouder/conversation-system": "^0.0.69",
    "@dataclouder/core-components": "^0.0.15",
    "@dataclouder/dc-mic": "^0.0.2",
    "@dataclouder/lessons": "^0.0.22",
    "@dataclouder/storage-uploader": "^0.0.18",
    "@ionic/angular": "^8.0.0",
    "@jcesarmobile/ssl-skip": "0.4.0",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "@primeng/themes": "^19.0.5",
    "firebase": "^10.8.0",
    "force": "^0.0.3",
    "ionicons": "^7.2.1",
    "ngx-image-cropper": "^9.0.0",
    "npm-check-updates": "^17.1.13",
    "primeicons": "^7.0.0",
    "primeng": "^19.0.5",
    "rxjs": "~7.8.0",
    "swiper": "^11.1.14",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.3",
    "@angular-eslint/builder": "^19.0.2",
    "@angular-eslint/eslint-plugin": "^19.0.2",
    "@angular-eslint/eslint-plugin-template": "^19.0.2",
    "@angular-eslint/schematics": "^19.0.2",
    "@angular-eslint/template-parser": "^19.0.2",
    "@angular/cli": "^19.1.3",
    "@angular/compiler-cli": "^19.1.2",
    "@angular/language-service": "^19.1.2",
    "@capacitor/cli": "6.1.2",
    "@ionic/angular-toolkit": "^11.0.1",
    "@types/jasmine": "~5.1.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.1",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "jasmine-core": "~5.1.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.5.0"
  },
  "description": "An Ionic project"
}
